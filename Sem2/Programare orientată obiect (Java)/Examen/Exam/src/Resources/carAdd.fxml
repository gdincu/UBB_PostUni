
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Spinner?>
<VBox fx:controller="UI.Controller"
      xmlns:fx="http://javafx.com/fxml" alignment="center">

    <TextField fx:id="txtId" promptText="Car Id"/>
    <TextField fx:id="txtModel" promptText="Model"/>
    <TextField fx:id="txtKm" promptText="Km"/>
    <TextField fx:id="txtPrice" promptText="Price"/>

    <Button text="Add" fx:id="btnAdd" onAction="#btnAddCarClick" />
    <Button text="Cancel" fx:id="btnCancel" onAction="#btnBookingCancelClick" />
</VBox>
